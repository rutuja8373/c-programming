#include<stdio.h>
int main()
{
	int a[100],i,n,max,min;
	printf("enter limit:");
	scanf("%d",&n);
	printf("\n enter n no:");
	for(i=0;i<n;i++)
	{
		scanf("%d",&a[i]);
	}
	max=a[0];
	min=a[0];
	for(i=0;i<n;i++)
	{
		if(a[i]<min)
			{
				min=a[i];
			}
		if(a[i]>max)
		{
			max=a[i];
		}
	}
	printf("\n maximum no=%d",max);
	printf("\n minimum no=%d",min);
}
